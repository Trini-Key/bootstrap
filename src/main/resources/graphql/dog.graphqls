type Dog {
    id: ID!
    name: String!
    breed: String!
    origin: String!
}

type Query {
    findAllDogs: [Dog]!
    findAllBreed(breed: String!) :Dog!
    findAllName(name: String!): Dog!
    findBreedById(id: ID!) : Dog!
}

type Mutation {
    newDog(name: String!, breed: String!, origin: String!) : Dog!
    deleteDogBreed(breed: String!) : Boolean
    updateDogName(newName: String!, id:ID!) : Dog!
}